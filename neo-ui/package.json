{
  "name": "neo-ui",
  "version": "0.1.5",
  "author": "Nipun Amarasekara",
  "scripts": {
    "start": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "npm run build:lib && npm run build:style",
    "build:lib": "vue-cli-service build --target lib --name neo-ui src/index.js",
    "build:style": "postcss src/assets/tailwind.css -o dist/styles.css"
  },
  "main": "dist/neo-ui.common.js",
  "module": "dist/neo-ui.umd.min.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "autoprefixer": "^10",
    "postcss": "^8",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-cli-plugin-tailwind": "~3.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}